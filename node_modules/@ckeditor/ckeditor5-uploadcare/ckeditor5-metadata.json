{
    "plugins": [
        {
            "name": "Uploadcare",
            "className": "Uploadcare",
            "description": "Allows inserting images into the rich-text editor content and to the Uploadcare cloud.",
            "docs": "",
            "path": "src/uploadcare.js",
            "requires": [
                [
                    "ImageBlock",
                    "ImageInline"
                ],
                "ImageUpload"
            ],
            "htmlOutput": [
                {
                    "elements": [
                        "a",
                        "figure",
                        "img"
                    ],
                    "attributes": [
                        "data-uploadcare-resource-id"
                    ]
                }
            ]
        },
        {
            "name": "Uploadcare Image Edit",
            "className": "UploadcareImageEdit",
            "description": "Allows editing images.",
            "docs": "",
            "path": "src/uploadcareimageedit.js",
            "requires": [
                "Uploadcare"
            ],
            "uiComponents": [
                {
                    "name": "uploadcareImageEdit",
                    "type": "Button",
                    "iconPath": "@ckeditor/ckeditor5-icons/theme/icons/uploadcare-image-edit.svg"
                }
            ]
        }
    ]
}
